{
  "name": "node-express-starter",
  "version": "0.0.0",
  "discription": "node + express starter app.",
  "author": "ayo ajila",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "audit": "npm audit",
    "lint": "node node_modules/eslint/bin/eslint.js config routes bin/www app.js",
    "test": "node node_modules/jest/bin/jest.js",
    "outdated": "npm outdated",
    "dev": "cross-env NODE_ENV=development nodemon --max-http-header-size=40000 ./bin/www",
    "all": "npm-run-all --parallel lint outdated audit test dev",
    "start": "cross-env NODE_ENV=production node --max-http-header-size=40000 ./bin/www"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  }
}
